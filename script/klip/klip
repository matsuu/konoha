#!/usr/local/bin/konoha

int klipMain(String[] args);

void helloKlip()
{
	ins = new InputStream("./sys/hello.klip","r");
	foreach(String txt from ins) {
		OUT.println(txt);
	}
	ins.close();
}

//*-----main-----
int  main (String[] args)
{
 	if ( |args| > 1 ) { 
		eval('import "./src/check.k";');
		
		eval('import "./src/init.k";');
		eval('import "./src/import.k";');
		return klipMain(args); 
	} else {
		helloKlip();
		return 0;
}
